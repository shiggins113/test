<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd"
		
		default-autowire="byName" default-autowire-candidates="*" >

  <bean id="TicketStatus" class="io.pivotal.ticket.model.TicketStatus">
		<property name="ticketId" value="1"></property>
		<property name="timeElapsed" value="123"></property>
		<property name="status" value="open"></property>
		<property name="engineer" value="John"></property>
		<property name="messageSent" value="slack"></property>
	</bean>
	
	 <bean id="TicketStatus1" class="io.pivotal.ticket.model.TicketStatus">
		<property name="ticketId" value="2"></property>
		<property name="timeElapsed" value="1234"></property>
		<property name="status" value="new"></property>
		<property name="engineer" value="Stephen"></property>
		<property name="messageSent" value="email"></property>
	</bean>
	


</beans>
